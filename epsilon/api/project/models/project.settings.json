{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "name": "Project",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "slug": {
      "type": "string"
    },
    "isTop": {
      "type": "boolean"
    },
    "githubLink": {
      "type": "string"
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "Personal",
        "Client"
      ]
    },
    "clientName": {
      "type": "string"
    },
    "startDate": {
      "type": "date"
    },
    "endDate": {
      "type": "date"
    },
    "snapshots": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "authors": {
      "collection": "writer"
    },
    "content": {
      "type": "richtext"
    },
    "deployedLink": {
      "type": "string"
    }
  }
}
